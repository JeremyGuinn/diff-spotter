{
  "build": {
    "beforeDevCommand": "pnpm start",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist/diff-spotter/browser",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "diff-spotter",
  "version": "0.1.0",
  "identifier": "com.diff-spotter.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Diffspotter",
        "width": 1600,
        "height": 900,
        "dragDropEnabled": false
      }
    ]
  }
}
